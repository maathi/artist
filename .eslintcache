d) },\n  })\n\n  useEffect(() => {\n    if (!data) return\n    console.log(data)\n\n    if (!data.art) return\n    setArt(data.art)\n    console.log(\"my arts\", art)\n  }, [data])\n\n  if (loading) return <p>Loading...</p>\n  if (error) return <p>Error :(</p>\n\n  return (\n    <div>\n      <p>{art?.name}</p>\n      <img\n        src={`http://localhost:4000/${art?.pic}.png`}\n        style={{\n          backgroundColor: \"rgb(209, 209, 209)\",\n          border: \"3px solid black\",\n        }}\n        alt=\"\"\n      />\n\n      <p style={{ fontSize: \"18px\" }}>\n        <span>Created by : </span>\n        <Link to={`/users/${art?.owner.id}`} style={{ color: \"white\" }}>\n          {art?.owner.name}\n        </Link>\n      </p>\n      <p style={{ fontSize: \"18px\" }}>\n        <span>Price :</span>\n        <span style={{ color: \"white\" }}> {art?.price} million USD</span>\n      </p>\n      <p style={{ fontSize: \"18px\" }}>\n        <span>Description :</span>\n        <span style={{ color: \"white\" }}> {art?.description}</span>\n      </p>\n    </div>\n  )\n}\nexport default Art\n","/home/mathi/kumo/artist/src/components/art/new.jsx",["62","63"],"/home/mathi/kumo/artist/src/schema.ts",[],"/home/mathi/kumo/artist/src/components/art/canvas.jsx",["64","65"],"import { useState, useEffect } from \"react\"\n\nexport default function Canvas(props) {\n  let [p, setP] = useState()\n  let [ctx, setCtx] = useState()\n  let [canvas, setCanvas] = useState()\n  let [mouse, setMouse] = useState(\"up\")\n\n  useEffect(() => {\n    let canvas = document.getElementById(\"canvas\")\n    setCanvas(canvas)\n    let ctx = canvas.getContext(\"2d\")\n    setCtx(ctx)\n  }, [])\n\n  useEffect(() => {\n    if (!ctx) return\n\n    if (mouse == \"up\") {\n      ctx.beginPath()\n      return\n    }\n\n    ctx.lineWidth = props.lineWidth\n    ctx.lineCap = \"round\"\n    ctx.lineTo(p?.x, p?.y)\n    ctx.strokeStyle = props.color\n    ctx.stroke()\n  }, [p])\n\n  function handleMouseMove(e) {\n    var rect = canvas.getBoundingClientRect()\n    setP({\n      x: ((e.clientX - rect.left) / (rect.right - rect.left)) * canvas.width,\n      y: ((e.clientY - rect.top) / (rect.bottom - rect.top)) * canvas.height,\n    })\n  }\n\n  function dataURItoBlob(dataURI) {\n    var byteString = atob(dataURI.split(\",\")[1])\n    var mimeString = dataURI.split(\",\")[0].split(\":\")[1].split(\";\")[0]\n    var ab = new ArrayBuffer(byteString.length)\n    var ia = new Uint8Array(ab)\n    for (var i = 0; i < byteString.length; i++) {\n      ia[i] = byteString.charCodeAt(i)\n    }\n    var blob = new Blob([ab], { type: mimeString })\n    return blob\n  }\n\n  function handleMouseUp() {\n    setMouse(\"up\")\n    let data = canvas.toDataURL()\n    let file = dataURItoBlob(data)\n    props.setFile(file)\n  }\n\n  return (\n    <div>\n      <canvas\n        onMouseMove={(e) => handleMouseMove(e)}\n        onMouseDown={() => setMouse(\"down\")}\n        onMouseUp={() => handleMouseUp()}\n        onMouseLeave={() => setMouse(\"up\")}\n        id=\"canvas\"\n        width=\"600px\"\n        height=\"400px\"\n        style={{\n          border: \"1px solid black\",\n          display: \"block\",\n          margin: \"auto\",\n          backgroundColor: \"#eee\",\n          cursor: \"cell\",\n        }}\n      ></canvas>\n    </div>\n  )\n}\n",["66","67"],"/home/mathi/kumo/artist/src/components/nav.jsx",["68"],"import { Link } from \"react-router-dom\"\nimport logo from \"../img/artist.png\"\n\nimport \"../styles/navbar.css\"\nfunction Nav(props) {\n  function logout() {\n    localStorage.clear()\n    window.location.href = \"/login\"\n  }\n\n  return (\n    <ul id=\"nav\">\n      <li>\n        <img id=\"logo\" src={logo} alt=\"\" />\n      </li>\n      <li>\n        <Link to=\"/paintings/new\">+</Link>\n      </li>\n      <li>\n        <Link to=\"/paintings\">paintings</Link>\n      </li>\n      <li>\n        <img\n          id=\"nav-pic\"\n          src={`http://localhost:4000/${localStorage.getItem(\"photo\")}`}\n          alt=\"\"\n        />\n\n        <Link to={\"/users/\" + localStorage.getItem(\"id\")}>\n          {localStorage.getItem(\"name\")}\n        </Link>\n      </li>\n      <li>\n        <a onClick={logout}>logout</a>\n      </li>\n    </ul>\n  )\n}\n\nexport default Nav\n","/home/mathi/kumo/artist/src/components/user/login.jsx",["69"],"/home/mathi/kumo/artist/src/components/user/register.jsx",["70","71"],"import { useMutation, gql } from \"@apollo/client\"\nimport { useEffect, useState } from \"react\"\nimport { UserType } from \"../../schema\"\nimport { Link } from \"react-router-dom\"\n\nconst ADD_USER = gql`\n  mutation AddUser($name: String, $password: String) {\n    addUser(name: $name, password: $password) {\n      id\n      name\n      password\n    }\n  }\n`\nfunction Register() {\n  let [user, setUser] = useState()\n  let [name, setName] = useState(\"\")\n  let [password, setPassword] = useState(\"\")\n  const [addUser, { data }] = useMutation(ADD_USER)\n\n  useEffect(() => {\n    if (!data) return\n    if (!data.addUser) return\n\n    setUser(data.addUser)\n    console.log(\"my user: \", user)\n    // localStorage.setItem(\"userId\", data.addUser.id)\n    // localStorage.setItem(\"userName\", user?.name)\n  }, [data])\n\n  useEffect(() => {\n    if (!user) return\n\n    localStorage.setItem(\"id\", user.id.toString())\n    localStorage.setItem(\"name\", user.name.toString())\n    window.location.href = \"/paintings\"\n  }, [user])\n\n  return (\n    <div>\n      <input\n        type=\"text\"\n        placeholder=\"name\"\n        value={name}\n        onChange={(e) => setName(e.target.value)}\n      />\n      <input\n        type=\"text\"\n        placeholder=\"password\"\n        value={password}\n        onChange={(e) => setPassword(e.target.value)}\n      />\n      <button onClick={() => addUser({ variables: { name, password } })}>\n        Register\n      </button>\n      <span>\n        already have an account? <Link to=\"/login\">login</Link>\n      </span>\n      {user ? <p>{user.name} was added!</p> : \"\"}\n    </div>\n  )\n}\nexport default Register\n","/home/mathi/kumo/artist/src/components/user/user.jsx",["72","73","74"],"/home/mathi/kumo/artist/src/index.jsx",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":1,"column":25,"nodeType":"81","messageId":"82","endLine":1,"endColumn":36},{"ruleId":"83","severity":1,"message":"84","line":32,"column":6,"nodeType":"85","endLine":32,"endColumn":12,"suggestions":"86"},{"ruleId":"79","severity":1,"message":"80","line":1,"column":25,"nodeType":"81","messageId":"82","endLine":1,"endColumn":36},{"ruleId":"83","severity":1,"message":"87","line":37,"column":6,"nodeType":"85","endLine":37,"endColumn":12,"suggestions":"88"},{"ruleId":"89","severity":1,"message":"90","line":39,"column":11,"nodeType":"91","messageId":"92","endLine":41,"endColumn":12},{"ruleId":"89","severity":1,"message":"90","line":48,"column":11,"nodeType":"91","messageId":"92","endLine":50,"endColumn":12},{"ruleId":"93","severity":1,"message":"94","line":22,"column":15,"nodeType":"95","messageId":"96","endLine":22,"endColumn":17},{"ruleId":"83","severity":1,"message":"97","line":32,"column":6,"nodeType":"85","endLine":32,"endColumn":9,"suggestions":"98"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"99","severity":1,"message":"100","line":34,"column":9,"nodeType":"101","endLine":34,"endColumn":29},{"ruleId":"102","severity":1,"message":"103","line":3,"column":10,"nodeType":"81","messageId":"82","endLine":3,"endColumn":18},{"ruleId":"102","severity":1,"message":"103","line":3,"column":10,"nodeType":"81","messageId":"82","endLine":3,"endColumn":18},{"ruleId":"83","severity":1,"message":"104","line":29,"column":6,"nodeType":"85","endLine":29,"endColumn":12,"suggestions":"105"},{"ruleId":"102","severity":1,"message":"103","line":4,"column":10,"nodeType":"81","mess